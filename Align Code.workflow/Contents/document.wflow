					<string>lines = ARGF.readlines

indent = 0

lines.reverse.each { |line|
  space = line[/^\s+[^\s]/]
  next if space.nil?
  indent = space.length - 1
  break
}

if indent == 0
  puts lines
  exit
end

alignment = Regexp.new("^\\s{1,#{indent}}")

lines.lazy.map { |l| l.sub(alignment, "") }.each(&amp;:display)
puts
